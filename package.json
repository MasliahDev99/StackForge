    "start": "node dist/run.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "typescript",
    "react",
    "tailwind",
    "vite",
    "scaffolding",
    "stackforge"
  ],
  "author": "Felipe Masliah",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@eslint/json": "^0.13.1",
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@octokit/rest": "^22.0.0",
    "chalk": "^5.5.0",
    "clak": "^1.0.2",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.8",
    "execa": "^9.6.0",
    "ora": "^8.2.0"
  }
