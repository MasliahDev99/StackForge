{
  "name": "stackforge-cli",
  "version": "1.0.0",
  "description": "CLI para generar proyectos React + TailwindCSS con arquitectura limpia y automatizaci√≥n completa.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stackforge": "./bin/index.js"
  },
  "files": [
    "bin",
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "dev": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "cli",
    "typescript",
    "react",
    "tailwind",
    "vite",
    "scaffolding",
    "stackforge"
  ],
  "author": "Felipe Masliah",
  "license": "MIT",
  "packageManager": "pnpm@10.13.1",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@eslint/json": "^0.13.1",
    "@socketsecurity/cli": "^1.0.85",
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^24.2.0",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "chalk": "^5.5.0",
    "clak": "^1.0.2",
    "cli-progress": "^3.12.0",
    "execa": "^9.6.0",
    "ora": "^8.2.0"
  }
